{
    "name": "rizkhal/lara-tabler",
    "description": "Laravel using tabler",
    "type": "package",
    "keywords": [
        "tabler",
        "laravel"
    ],
    "homepage": "https://github.com/codegoen/lara-tabler",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Rizkhal Lamaau",
            "email": "lamaaurizkhal@gmail.com",
            "homepage": "https://rizkhal.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "pestphp/pest": "^0.1.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rizkhal\\Tabler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rizkhal\\Tabler\\TablerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
