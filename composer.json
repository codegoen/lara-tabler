{
    "name": "rizkhal/lara-tabler",
    "description": "Laravel CRUD Generator using tabler",
    "type": "package",
    "keywords": [
        "tabler",
        "laravel",
        "crud generator"
    ],
    "homepage": "https://github.com/codegoen/lara-tabler",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Rizkhal Lamaau",
            "email": "lamaaurizkhal@gmail.com",
            "homepage": "https://rizkhal.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0",
        "laravel/ui": "^2.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "3.8.*|4.*|5.*",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Rizkhal\\Tabler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rizkhal\\Tabler\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rizkhal\\Tabler\\TablerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
